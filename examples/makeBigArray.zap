//create an array from 0 to 100
var a = [0.0];
??(var b = [1.0];b < [101.0];b = b + [1.0]){
    a++b;
}

//print the array
!a;

//print the sum of the elemets of the array
!+a;

//print the last element of the array
!a(#a-[1]);

//powers of two
var divByTwo = [0.0];

var aCop = a;
?!(|(aCop==[2.0])){
    divByTwo++(a(%(aCop==[2.0])));
    aCop = aCop/[2.0];
}

//print powers of two
!divByTwo;